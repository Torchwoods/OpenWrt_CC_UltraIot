config system
	option hostname	YunYinIot
	option timezone	CST-8
	option conloglevel '5'
    option cronloglevel '1'
    option kconloglevel '1'
    option zonename 'Asia/Shanghai'

config timeserver ntp
	list server	0.openwrt.pool.ntp.org
	list server	1.openwrt.pool.ntp.org
	list server	2.openwrt.pool.ntp.org
	list server	3.openwrt.pool.ntp.org
	option enabled 1
	option enable_server 0

config button
        option button 'BTN_1'
        option action 'released'
        option handler "/bin/ledblink 'mediatek:green:wifi' 'slow'"
        option min '0'
        option max '5'

config button
        option button 'BTN_2'
        option action 'released'
        option handler "/bin/ledblink 'mediatek:green:wifi' 'mid'"
        option min '0'
        option max '5'

config button
        option button 'BTN_3'
        option action 'released'
        option handler "/bin/ledblink 'mediatek:green:wifi' 'fast'"
        option min '0'
        option max '5'
